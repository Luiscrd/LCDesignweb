
function pasaradmin(boton){
	$(boton).css('color','red')
	}
	
function saliradmin(boton){
	$(boton).css('color','white')
	}
	
function pasaradmin2(boton){
	$(boton).css('color','red')
	$(boton).css('border-color','red')
	}
	
function saliradmin2(boton){
	$(boton).css('color','black')
	$(boton).css('border-color','gray')
	}
	
function pasaradmin3(boton){
	$(boton).css('color','red')
	$(boton).css('border-color','red')
	}
	
function saliradmin3(boton){
	$(boton).css('color','green')
	$(boton).css('border-color','gray')
	}
	
function pasaradmin4(boton){
	$(boton).css('color','white')
	$(boton).css('border-color','red')
	$(boton).css('text-shadow','0px 5px 7px #FF0000, 0px -5px 7px #FF0000, 5px 0px 7px #FF0000, -5px 0px 7px #FF0000')
	}
	
function saliradmin4(boton){
	$(boton).css('color','red')
	$(boton).css('border-color','gray')
	$(boton).css('text-shadow','0px 5px 7px #000000, 0px -5px 7px #000000, 5px 0px 7px #000000, -5px 0px 7px #000000')
	}
	
