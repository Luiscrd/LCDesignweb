function pasar(boton){
	$(boton).attr('class','botonescabe2')
	}
	
function salir(boton){
	$(boton).attr('class','botonescabe')
	}
	
function pasar2(boton){
	$(boton).css('color','white')
	}
	
function salir2(boton){
	$(boton).css('color','#730073')
	}
	
function pasar4(boton){
	$(boton).css('color','black')
	$(boton).css('text-shadow','-1px 2px 10px white')
	}
	
function salir4(boton){
	$(boton).css('color','white')
	$(boton).css('text-shadow','-1px 2px 10px #000000')
	}
	
function pasar5(boton){
	$(boton).css('background-color','#e5097f')
	}
	
function salir5(boton){
	$(boton).css('background-color','rgba(255, 255, 255, 0.3)')
	}
	
function pasar6(boton){
	$(boton).css('background-color','#e5097f')
	$(boton).css('color','white')
	}
	
function salir6(boton){
	$(boton).css('background-color','rgba(255, 255, 255, 0.8)')
	$(boton).css('color','#e5097f')
	}
	
function pasar7(boton){
	$(boton).css('color','white')
	$(boton).css('text-shadow','5px 0 5px #7e71b1, -5px 0 5px #7e71b1, 0 5px 5px #7e71b1, 0 -5px 5px #7e71b1')
	}
	
function salir7(boton){
	$(boton).css('color','#7e71b1')
	$(boton).css('text-shadow','0 0 0 #000000')
	}
	

